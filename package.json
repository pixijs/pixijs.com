{
    "name": "pixi-docusaurus",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "prebuild": "npm run generate-content",
        "build": "docusaurus build",
        "clear": "docusaurus clear",
        "predeploy": "npm run generate-content",
        "deploy": "docusaurus deploy",
        "docusaurus": "docusaurus",
        "lint": "eslint . --ext .js,.mjs,.jsx,.ts,.tsx --max-warnings=0",
        "lint:fix": "eslint . --fix --ext .js,.mjs,.jsx,.ts,.tsx",
        "prettier:check": "prettier --check .",
        "prettify": "prettier --write .",
        "serve": "docusaurus serve",
        "prestart": "npm run generate-content",
        "start": "cross-env PIXI_VERSION=dev docusaurus start",
        "swizzle": "docusaurus swizzle",
        "types": "tsc --noEmit",
        "write-heading-ids": "docusaurus write-heading-ids",
        "write-translations": "docusaurus write-translations",
        "generate-content": "node ./scripts/generate-tutorial-docs.js",
        "update-version": "node ./scripts/update-version.js",
        "prepare": "husky install"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx,mjs}": [
            "prettier --write",
            "eslint --cache --fix"
        ],
        "*.{css,scss}": "prettier --write"
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@codesandbox/sandpack-react": "^2.19.10",
        "@codesandbox/sandpack-themes": "^2.0.21",
        "@docusaurus/core": "~3.6.3",
        "@docusaurus/faster": "^3.6.3",
        "@docusaurus/plugin-client-redirects": "~3.6.3",
        "@docusaurus/plugin-content-blog": "~3.6.3",
        "@docusaurus/plugin-google-analytics": "~3.6.3",
        "@docusaurus/preset-classic": "~3.6.3",
        "@easyops-cn/docusaurus-search-local": "^0.46.1",
        "@mdx-js/react": "^3.1.0",
        "@monaco-editor/react": "^4.6.0",
        "@pixi/webdoc-docusaurus-template": "^1.0.1",
        "classnames": "^2.5.1",
        "clsx": "^2.1.1",
        "docusaurus-plugin-sass": "^0.2.6",
        "pixi.js": "^8.6.6",
        "prism-react-renderer": "^2.4.1",
        "ramda": "^0.30.1",
        "raw-loader": "^4.0.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-intersection-observer": "^9.13.1",
        "sass": "^1.83.0"
    },
    "devDependencies": {
        "@docusaurus/module-type-aliases": "^3.6.3",
        "@docusaurus/tsconfig": "3.6.3",
        "@docusaurus/types": "^3.6.3",
        "@pixi/eslint-config": "^4.0.1",
        "@types/ramda": "^0.30.2",
        "compare-versions": "^6.1.1",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.7",
        "eslint": "^8.57.1",
        "eslint-config-standard-jsx": "^11.0.0",
        "eslint-config-standard-with-typescript": "^30.0.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^4.6.2",
        "glob": "^10.4.5",
        "husky": "^8.0.3",
        "inquirer": "^8.2.6",
        "lint-staged": "^13.3.0",
        "prettier": "^3.4.2",
        "recast": "^0.23.9",
        "semver": "^7.6.3",
        "typescript": "^5.7.2"
    },
    "engines": {
        "node": ">=20"
    }
}
